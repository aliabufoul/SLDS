    "import numpy as np\n",
    "\n",
    "x = np.matrix([[1,2,6],[5,10,0],[0,10,10]])\n",
    "A = np.matrix([0,2,1])\n",
    "y = x * A.transpose()\n",
    "\n",
    "print(\"A = \")\n",
    "print(A)\n",
    "print()\n",
    "\n",
    "print(\"x = \")\n",
    "print(x)\n",
    "print()\n",
    "\n",
    "print(\"y = \")\n",
    "print(y)\n",
    "print()\n",
    "\n",
    "reg_A = np.linalg.inv(x.transpose()*x)*x.transpose()*y\n",
    "print(reg_A)\n"
   ]
